<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% categories.forEach(category => { %>
        <li>
          <a href="<%= category.url %>"><%= category.name %></a>
        </li>
      <% }) %>
    </ul>
  </body>
</html>
